{
  "name": "my-stati-site",
  "version": "1.0.0",
  "description": "A minimal Stati site",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "stati dev",
    "build": "stati build",
    "preview": "stati preview",
    "clean:dist": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\"",
    "clean:cache": "node -e \"require('node:fs').rmSync('.stati',{recursive:true,force:true})\"",
    "clean": "npm run clean:dist && npm run clean:cache"
  },
  "keywords": [
    "static-site",
    "stati",
    "minimal"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@stati/cli": "^1.9.0",
    "@stati/core": "^1.15.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
